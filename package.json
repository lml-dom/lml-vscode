{
    "name": "lml",
    "displayName": "lml",
    "description": "LML syntax parser",
    "version": "0.4.0",
    "publisher": "gvarsanyi",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/lml-dom/lml-vscode.git"
    },
    "contributes": {
        "languages": [{
            "id": "lml",
            "aliases": ["LML", "lml"],
            "extensions": [".lml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lml",
            "scopeName": "source.lml",
            "path": "./syntaxes/lml.tmLanguage.json"
        }]
    }
}